{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug StackTrace",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/StackTrace",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [
        { "name": "DEBUGINFOD_URLS", "value": "https://debuginfod.ubuntu.com" },
        { "name": "DEBUGINFOD_PROGRESS", "value": "1" }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "CMake Build"
    }
